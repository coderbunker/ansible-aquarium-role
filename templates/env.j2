APP_NAME=Laravel
APP_ENV={{ aquarium_env }}
APP_KEY={{ aquarium_cle }}
APP_DEBUG={{ aquarium_debogage }}
APP_URL=http://{{ aquarium_domaine }}

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST={{ aquarium_bd_hote }}
