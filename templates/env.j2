MAIL_ENCRYPTION={{ aquarium_smtp_ssl }}
MAIL_FROM_ADDRESS={{ aquarium_exp_courriel }}
MAIL_FROM_NAME={{ aquarium_exp_nom }}

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
